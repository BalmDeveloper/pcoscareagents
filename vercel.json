{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/frontend/_next/static/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/frontend/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend"
    }
  ]
}
